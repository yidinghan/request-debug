{
  "extends": "airbnb",
  "globals": {
    "it": true,
    "before": true,
    "beforeEach": true,
    "describe": true
  },
  "env": {
    "es6": true
  },
  "rules": {
    "no-underscore-dangle": 0,
    "prefer-destructuring": 0,
    "no-param-reassign": 0,
    "func-names": 0,
    "no-plusplus": 0,
    "comma-dangle": ["error", "never"]
  },
  "plugins": ["react", "jsx-a11y", "import"]
}
